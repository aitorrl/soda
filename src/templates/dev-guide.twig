  <h4>Componentes</h4>
  <ul>
    <li><a href="components-list-template.html">Todos los componentes</a></li>
    {% for value in components %}
      <li><a href="01-components/{{value.dirname}}/{{ value.filename }}.html">{{ value.filename }}</a></li>
